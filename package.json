{
  "name": "static-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "start": "run-p serve dev",
    "dev": "run-s clean:dist watch",
    "build": "run-s clean:dist copy imagemin build:* clean:sourcemaps",
    "build:styles": "run-s styles cleancss",
    "build:scripts": "webpack --mode=production",
    "build:html": "run-p pug ejs",
    "clean:dist": "del-cli -f \"dist/**\" \"!dist/**/.*\" \"!dist/wp\"",
    "clean:sourcemaps": "del-cli -f \"dist/**/*.map\"",
    "watch": "run-p watch:*",
    "watch:styles": "onchange -i \"src/assets/scss\" -- npm run styles",
    "watch:scripts": "npm run scripts -- -w",
    "watch:pug": "onchange -i \"src/**/*.pug\" -- npm run pug",
    "watch:ejs": "onchange -i \"src/**/*.ejs\" -- npm run ejs",
    "watch:images": "onchange -i \"src/assets/images/**/*.*\" -- npm run imagemin",
    "watch:copy": "onchange -i \"src/**/!(*.js|*.scss|*.pug|*.ejs)\" -e \"src/assets/images/*.*\" -- npm run copy",
    "serve": "browser-sync start -c bs-config.js",
    "styles": "run-s scss autoprefixer",
    "scss": "sass --embed-sources src/assets/scss/app.scss:dist/assets/css/app.css",
    "autoprefixer": "postcss dist/assets/css/app.css -u autoprefixer -o dist/assets/css/app.css -m",
    "cleancss": "cleancss -o dist/assets/css/app.css dist/assets/css/app.css",
    "scripts": "webpack --mode=development",
    "pug": "pug src -o dist -P",
    "ejs": "ejs-cli --base-dir src \"/**/{_tpl_,!(_)}*.ejs\" -o dist",
    "imagemin": "node imagemin.js",
    "copy": "copyfiles -e \"src/assets/images/*.*\" -u 1 \"src/**/!(*.js|*.scss|*.pug|*.ejs)\" dist"
  },
  "browserslist": [
    "last 2 version"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "autoprefixer": "^10.4.1",
    "babel-loader": "^8.2.3",
    "browser-sync": "^2.27.7",
    "clean-css-cli": "^5.5.0",
    "copyfiles": "^2.4.1",
    "core-js": "^3.20.2",
    "del-cli": "^4.0.1",
    "ejs-cli": "^2.2.3",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "imagemin-cli": "^7.0.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-keep-folder": "^5.3.2",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^9.0.0",
    "jquery": "^3.6.0",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.5.1",
    "pug-cli": "github:pugjs/pug-cli#master",
    "sass": "^1.46.0",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-scss": "^5.0.2",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  }
}
